{
    "name": "Record Migrator URL",
    "version": "0.1",
    "description": "This tool is to migrate the records within the same enviroment.",
	"permissions": ["activeTab", "declarativeContent", "storage", "cookies", "*://*.salesforce.com/", "*://*.restcountries.eu/"],
    "background": {
      "scripts": ["configuration.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "screen.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
}